\r\n.form__input {\r\n  padding: 4px;\r\n}\r\n\r\n.nav-box {\r\n  border: 1px solid $color-grey-light-3;\r\n}\r\n\r\n.logo:visited,\r\n:link {\r\n  text-decoration: none;\r\n  color: inherit;\r\n  padding: 1rem 2rem;\r\n}\r\n\r\n.bookmarks {\r\n  cursor: pointer;\r\n}\r\n\r\n// RESULTS\r\n\r\n.preview {\r\n  height: 20%;\r\n  min-height: 10vh;\r\n  display: grid;\r\n  grid-template-columns: 25% 55% 20%;\r\n  border-bottom: 1px solid $color-grey-light-3;\r\n  font-size: 1.4rem;\r\n  cursor: pointer;\r\n  transition: all 0.2s ease;\r\n\r\n  &:first-child {\r\n    margin-top: 2rem;\r\n  }\r\n  &__marker {\r\n    @include gridCentering;\r\n  }\r\n\r\n  &__info {\r\n    // @include gridCentering;\r\n    align-self: center;\r\n\r\n    &--name {\r\n    }\r\n\r\n    &--address {\r\n      font-size: 1rem;\r\n    }\r\n  }\r\n\r\n  &:hover {\r\n    background-color: $color-grey-light-3;\r\n    transform: translateY(-1px);\r\n  }\r\n\r\n  &__bookmark--icon {\r\n    height: 3rem;\r\n    width: 3rem;\r\n    @include gridCentering;\r\n    transition: all 0.1s;\r\n    &:hover {\r\n      transform: scale(1.1);\r\n    }\r\n  }\r\n}\r\n\r\n// Button\r\n\r\n.btn {\r\n  padding: 0.5rem 2rem;\r\n  border: 1px solid $color-grey-light-3;\r\n  cursor: pointer;\r\n  overflow: hidden;\r\n  border-radius: 3px;\r\n  &:focus {\r\n    outline: none;\r\n  }\r\n\r\n  &:hover {\r\n    transform: translateY(-2px);\r\n    @include boxShadow;\r\n  }\r\n  &:active {\r\n    transform: translateY(-1px);\r\n    box-shadow: 0 0.5rem 1rem rgba($color-black, 0.2);\r\n  }\r\n}\r\n\r\n// Bookmarks\r\n\r\n.bookmarks {\r\n  padding: 1rem 5rem;\r\n  position: relative;\r\n  align-items: stretch;\r\n  @include boxShadow;\r\n  border-radius: 3px;\r\n\r\n  &__list {\r\n    visibility: hidden;\r\n    opacity: 0;\r\n    width: 100%;\r\n    font-size: 1.6rem;\r\n    background-color: $color-grey-light-2;\r\n    position: absolute;\r\n    top: 6.3rem;\r\n    right: 0;\r\n    list-style: none;\r\n    transition: all 0.3s;\r\n    z-index: 2;\r\n    &--name {\r\n      padding: 1rem 2rem;\r\n\r\n      width: 100%;\r\n      &:hover {\r\n        background-color: $color-grey-light-3;\r\n      }\r\n    }\r\n  }\r\n\r\n  &:hover &__list {\r\n    visibility: visible;\r\n    opacity: 1;\r\n  }\r\n\r\n  &__preview {\r\n    padding: 1rem 2rem;\r\n    transition: all 0.2s ease;\r\n    &--name {\r\n    }\r\n    &--address {\r\n      font-size: 1rem;\r\n    }\r\n\r\n    &:hover {\r\n      background-color: $color-grey-light-3;\r\n      transform: translateY(-1px);\r\n    }\r\n  }\r\n}\r\n\r\n.spinner {\r\n  margin: 5rem auto;\r\n  text-align: center;\r\n  z-index: 99;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  // position: absolute;\r\n  // top: 30%;\r\n  // left: 50%;\r\n  // transform: translate(-50%, -50%);\r\n\r\n  svg {\r\n    height: 6rem;\r\n    width: 6rem;\r\n    animation: rotate 0.5s infinite linear;\r\n  }\r\n}\r\n\r\n@keyframes rotate {\r\n  0% {\r\n    transform: rotate(0);\r\n  }\r\n\r\n  100% {\r\n    transform: rotate(360deg);\r\n  }\r\n}\r\n\r\n.hidden {\r\n  display: none;\r\n}\r\n\r\n.markerbg {\r\n  // float: left;\r\n  // position: absolute;\r\n  width: 36px;\r\n  height: 37px;\r\n  // margin: 10px 0 0 10px;\r\n  background: url(https://t1.daumcdn.net/localimg/localimages/07/mapapidoc/marker_number_blue.png)\r\n    no-repeat;\r\n}\r\n.marker_1 {\r\n  background-position: 0 -10px;\r\n}\r\n.marker_2 {\r\n  background-position: 0 -56px;\r\n}\r\n.marker_3 {\r\n  background-position: 0 -102px;\r\n}\r\n.marker_4 {\r\n  background-position: 0 -148px;\r\n}\r\n.marker_5 {\r\n  background-position: 0 -194px;\r\n}\r\n.marker_6 {\r\n  background-position: 0 -240px;\r\n}\r\n.marker_7 {\r\n  background-position: 0 -286px;\r\n}\r\n.marker_8 {\r\n  background-position: 0 -332px;\r\n}\r\n.marker_9 {\r\n  background-position: 0 -378px;\r\n}\r\n.marker_10 {\r\n  background-position: 0 -423px;\r\n}\r\n.marker_11 {\r\n  background-position: 0 -470px;\r\n}\r\n.marker_12 {\r\n  background-position: 0 -516px;\r\n}\r\n.marker_13 {\r\n  background-position: 0 -562px;\r\n}\r\n.marker_14 {\r\n  background-position: 0 -608px;\r\n}\r\n.marker_15 {\r\n  background-position: 0 -654px;\r\n}\r\n","src\\sass\\_layout.scss":".container {\r\n  height: 80vh;\r\n  width: 80vw;\r\n  min-height: 50rem;\r\n  min-width: 99rem;\r\n  background-color: $color-grey-light-1;\r\n  border-radius: 1rem;\r\n}\r\n\r\n.header {\r\n  font-size: 2rem;\r\n  height: 15%;\r\n  box-shadow: 0 0.7rem 1rem rgba($color-black, 0.1);\r\n\r\n  .nav {\r\n    height: 100%;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: space-around;\r\n    padding: 0 3rem;\r\n  }\r\n}\r\n.content {\r\n  height: 85%;\r\n  display: flex;\r\n\r\n  .results {\r\n    overflow: hidden;\r\n    height: 100%;\r\n    width: 25%;\r\n    border-right: solid 1px $color-black;\r\n\r\n    &-container {\r\n      list-style: none;\r\n      height: 80%;\r\n    }\r\n\r\n    &-pagination {\r\n      height: 15%;\r\n      display: flex;\r\n      align-items: center;\r\n      justify-content: space-around;\r\n    }\r\n  }\r\n\r\n  .map-container {\r\n    height: 100%;\r\n    width: 75%;\r\n    position: relative;\r\n\r\n    #map {\r\n      width: 100%;\r\n      height: 100%;\r\n    }\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"c5b5c532c6dc09f22c72cbe237ba4604","cacheData":{"env":{}}}